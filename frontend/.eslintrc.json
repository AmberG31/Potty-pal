{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "plugin:react/recommended",
    "airbnb"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react", "prettier"],
  "rules": {
    "no-underscore-dangle": ["error", { "allow": ["_id"] }],
    "react/jsx-props-no-spreading": "off",
    "comma-dangle": "off",
    "no-console": "off"
  }
}
